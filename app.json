{
  "expo": {
    "name": "NearByMess",
    "slug": "nearbymess",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "nearbymess",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nearbymess.app"
    },
    "android": {
      "package": "com.nearbymess.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "011695a2-4b2d-452c-9c60-aad993b4f64d"
      }
    },
    "owner": "ionede"
  }
}
